-- Data for Name: coin_tables; Type: TABLE DATA; Schema: public; Owner: jb
--

COPY public.coin_tables (id, name, description) FROM stdin;
1	coins	Main collection, for tracking coins of the lowest value
2	third_zloty	Collection of coins of third zloty (PRL, 1949 - 1994)
3	rubles	Collection of 1 ruble coins from soviet republic (1961 - 1991)
4	coin_diameters	Diameters of the lowest value coins for collection planning purposes
5	coin_box_positions	Position of the main collection coins in a box
\.


